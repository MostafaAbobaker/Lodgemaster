            a(href="OrganizationDetails.html") 
              .icon 
                i.fa-solid.fa-minus
              span.text Organization Details   
      li(class=titlePage == 'Inspection' ? "active" : " ")
        a(href="SetupOrganization.html")
          .icon
            include ../../assets/images/Svg/Settings3.svg
          span.text Setup Organization
      li(class=titlePage == 'RoomsStatus' ? "icon-room active" : " ")
        a(href="RoomsStatus.html")
          .icon
            include ../../assets/images/Svg/Rooms.svg
          span.text Rooms Status
      li(class=titlePage == 'RoomsStatusV2' ? "icon-room active" : " ")
        a(href="RoomsStatusV2.html")
          .icon
