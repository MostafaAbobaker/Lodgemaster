          .icon 
            include ../../assets/images/Svg/Settings3.svg
          span.text Settings
        #collapseSettings.collapse
            
            a(href="Departments.html") 
              .icon 
                i.fa-solid.fa-minus  
              span.text  Department
            a(href="Users.html") 
              .icon 
                i.fa-solid.fa-minus
              span.text User
            a(href="setupRoom.html") 
              .icon 
                i.fa-solid.fa-minus
              span.text Inspection 
            a(href="ProfileSettings.html") 
              .icon 
                i.fa-solid.fa-minus
