//- Start Aside Bar
aside.aside-bar
  .header 
    a.image(href="CategoryHotel.html")  
      img.large-logo(src="./assets/images/logoWhite.png", alt="" )
      img.small-logo(src="./assets/images/main-logo.png", alt="" )
  .body-aside-bar
    ul#collapseGroup
      li(class=titlePage == 'Dashboard' ? "active" : " ")
        a(href="LayoutDashboard.html")
          .icon 
            include ../../assets/images/Svg/OverView.svg
          span.text  Dashboard
      li(class=titlePage == 'Service Orders' ? "active" : " ")
        a(href="ServiceOrders.html")
          .icon 
            include ../../assets/images/Svg/ServiceOrdersAside.svg
          span.text Service Orders
      li(class=titlePage == 'Items' ? "active" : " ")
        a(href="Items.html")
          .icon 
            include ../../assets/images/Svg/ItemAside.svg
          span.text Items
      
      li(class=titlePage == 'Inspection' ? "active" : " ")
        a(href="Inspection.html")
          .icon
            include ../../assets/images/Svg/RoomsInspection.svg
          span.text Rooms Inspection
      li(class=titlePage == 'Notifications' ? "active" : " ")
        a(href="Notifications.html")
          .icon 
            include ../../assets/images/Svg/Notifications.svg
          span.text Notifications
          span.Notifi 9
      li(class=titlePage == 'Reports' ? "active item-collapse" : "item-collapse  ")
