- var titlesPage = 'Reports Inspection';
doctype html
html(lang="en")
  head
    include includes/head.pug
  body 
    .Dashboard-home
      include includes/headerNavBar.pug
      include includes/asideBar.pug
      //- start Content category hotel
      section.content-category-hotel 
        //- Start Filter
        .container-fluid 
          .row
            .col-lg-12 
              
            .col-lg-12.pt-3
              header.header-filter.flex-column
                h4.fw-bold Reports
                .container-fluid
                  .row
                    .col-lg-3.col-md-6.col-sm-12.mb-3
                      .form-group
                        label.mb-2(for='exampleFormControlSelect1') Period Date
                        select.form-select(aria-label='Default select example')
                          option(selected='') This Today 
                          option(value='1') Before Month
                          option(value='2') Before Year
                          option(value='3') Before Week 
                          option(value='3') This Week

                    .col-lg-3.col-md-6.col-sm-12.mb-3
                      .form-group
                        label.mb-2(for='exampleFormControlSelect1') From Date
                        input.form-control.rounded-pill(type="date" )
                    .col-lg-3.col-md-6.col-sm-12.mb-3
                      .form-group
                        label.mb-2(for='exampleFormControlSelect1') To Date
                        input.form-control.rounded-pill(type="date" )
                    .col-lg-3.col-md-6.col-sm-12.mb-3
                      label.mb-2(for='exampleFormControlSelect1')  &nbsp;
                      .d-flex.justify-content-around
                        a.btn.btn-master.d-flex.align-items-center.rounded-pill.btn-Create-Order(href='CreateOrderGeneral.html') 
                          | Apply Filter
                        button.btn.btn-outline-dark.rounded-pill Export
                            i.fa-solid.fa-cloud-arrow-down.ms-2.fa-fw
                  //- a.btn.btn-master.d-flex.align-items-center.rounded-pill.btn-Create-Order(href='CreateOrderGeneral.html') 
                  //-   i.fa-solid.fa-plus.me-2
                  //-   | Create Order
        //- End Filter
        //- Start Load Master page 
        #pageMasterLoad
          .HomeDashboard
            .container-fluid 
              .row           
                .col-lg-12.mb-4
                  .overview-Orders
                    .header-overview.mb-3.justify-content-start 
                      .title-head.mb-0  Inspection
                    
  
                    table.table.table-hover.table-responsive
                      thead
                        tr
                          th(scope='col') Inspection No.
                          th(scope='col') Location
                          th(scope='col') Inserter Name
                          th(scope='col') Date
                          th(scope='col') Status
                      tbody
                        tr
                          td(data-label="Location") Room 101
                          td(data-label="Accessories") Water, Blanket
                          td(data-label="Assigned to") 
                            .user-card.d-flex.align-items-center 
                              .image.me-1
                                img(src="./assets/images/photoUser.png", alt="" width="25")
                              .text Katef
                          td(data-label="Num Questions ") 5
                          td(data-label="Status" ) 
                            a(href="informationInspection.html") 
                              span.badge.bg-Completed.text-dark.rounded-pill Open
                        tr
                          td(data-label="Location") Room 101
                          td(data-label="Accessories") Water, Blanket
                          td(data-label="Assigned to") 
                            .user-card.d-flex.align-items-center 
                              .image.me-1
                                img(src="./assets/images/photoUser.png", alt="" width="25")
                              .text Katef
                          td(data-label="Num Questions ") 5
                          td(data-label="Status") 
                            span.badge.bg-Completed.text-dark.rounded-pill Completed
                        tr
                          td(data-label="Location") Room 101
                          td(data-label="Accessories") Water, Blanket
                          td(data-label="Assigned to") 
                            .user-card.d-flex.align-items-center 
                              .image.me-1
                                img(src="./assets/images/photoUser.png", alt="" width="25")
                              .text Katef
                          td(data-label="Num Questions ") 5
                          td(data-label="Status") 
                            span.badge.bg-process.text-dark.rounded-pill In process
                          
                        tr
                          td(data-label="Location") Room 101
                          td(data-label="Accessories") Water, Blanket
                          td(data-label="Assigned to") 
                            .user-card.d-flex.align-items-center 
                              .image.me-1
                                img(src="./assets/images/photoUser.png", alt="" width="25")
                              .text Katef
                          td(data-label="Num Questions ") 5
                          td 
                            span.badge.bg-Closed.text-dark.rounded-pill Closed
                          
                        tr
                          td(data-label="Location") Room 101
