                            span.badge.bg-Hold.text-dark.rounded-pill Hold
                         
                        
      
         
                    
                    .d-flex.justify-content-center 
                      nav.pagination-nav(aria-label='...')
                        ul.pagination
                          li.page-item.disabled
                            span.page-link 
                              i.fa-solid.fa-chevron-left
                          li.page-item
                            a.page-link(href='#') 1
                          li.page-item.active(aria-current='page')
                            span.page-link 2
                          li.page-item
                            a.page-link(href='#') 3
                          li.page-item
                            a.page-link(href='#') 
                              i.fa-solid.fa-chevron-right

            


      //- End Content category hotel
